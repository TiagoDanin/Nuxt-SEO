{
	"name": "nuxt-seo",
	"main": "lib/module.js",
	"preferGlobal": false,
	"version": "1.2.2",
	"description": "SEO / HTML Meta Tags Module for Nuxt.js",
	"author": {
		"email": "TiagoDanin@outlook.com",
		"name": "Tiago Danin",
		"url": "https://TiagoDanin.github.io"
	},
	"license": "MIT",
	"keywords": [
		"html-meta",
		"meta",
		"nuxt",
		"nuxt-module",
		"nuxtjs",
		"seo",
		"seo-meta",
		"seo-optimization",
		"seo-plugin",
		"social-media",
		"vue",
		"vuejs"
	],
	"scripts": {
		"start": "nuxt example",
		"test": "xo && ava"
	},
	"engines": {
		"node": ">= 8"
	},
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TiagoDanin/Nuxt-SEO.git"
	},
	"homepage": "https://TiagoDanin.github.io/Nuxt-SEO",
	"bugs": {
		"url": "https://github.com/TiagoDanin/Nuxt-SEO/issues"
	},
	"github": {
		"name": "Nuxt-SEO",
		"owner": "TiagoDanin"
	},
	"files": [
		"LICENSE",
		"README.md",
		"lib",
		"package.json"
	],
	"dependencies": {
		"debug": "^4.1.1"
	},
	"devDependencies": {
		"ava": "^2.4.0",
		"got": "^9.6.0",
		"nuxt": "^2.11.0",
		"vue": "^2.6.11",
		"xo": "^0.25.3"
	},
	"xo": {
		"ignores": [
			"lib/plugin.js"
		],
		"rules": {
			"quote-props": "warn",
			"eqeqeq": "warn",
			"array-callback-return": "warn",
			"require-atomic-updates": "warn",
			"no-else-return": "off",
			"ava/no-ignored-test-files": "off"
		},
		"semicolon": false
	},
	"documentation": "./docs.md",
	"ava": {
		"files": [
			"!test/fixtures/all.js"
		]
	}
}
